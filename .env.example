# FundPilot 环境变量配置示例

# 新闻API配置
VITE_NEWS_API_URL=https://api.example.com/v1
VITE_NEWS_API_KEY=your_news_api_key_here

# 市场数据API配置
VITE_MARKET_API_URL=https://api.marketdata.com/v1
VITE_MARKET_API_KEY=your_market_api_key_here

# 基金数据API配置
VITE_FUND_API_URL=https://api.funddata.com/v1
VITE_FUND_API_KEY=your_fund_api_key_here

# 应用配置
VITE_APP_TITLE=FundPilot
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# 功能开关
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_REAL_TIME_UPDATE=true
VITE_ENABLE_NEWS_TICKER=true

# 第三方服务
VITE_ANALYTICS_ID=your_analytics_id
VITE_SENTRY_DSN=your_sentry_dsn